<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-info">
      <div class="panel-body" id="table" >
        <%#= render "table", tags: @tags %>
        <ol class='sortable_tree' data-max_levels="5" data-rebuild_url="<%= rebuild_tags_url %>" >
          <%= sortable_tree @tags %>
        </ol>
      </div>
    </div>
  </div>
</div>
<div id="modal-window" class="modal fade" role="dialog"></div>